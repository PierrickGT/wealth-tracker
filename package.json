{
    "name": "WealthTracker",
    "version": "1.0.0",
    "description": "A simple interface to track my wealth",
    "main": "index.js",
    "scripts": {
        "format-all": "./node_modules/.bin/prettier --write **/*.js",
        "format-watch": "onchange '**/*.js' -- prettier --write {{changed}}",
        "lint": "lynt",
        "start": "parcel index.html",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format-all && npm run lint"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "antd": "3.6.5",
        "bizcharts": "3.1.10",
        "react": "16.4.1",
        "react-dom": "16.4.1",
        "react-router-dom": "4.3.1",
        "styled-components": "3.3.3"
    },
    "devDependencies": {
        "husky": "1.0.0-rc.13",
        "lynt": "0.4.0",
        "onchange": "4.0.0",
        "parcel-bundler": "1.9.4",
        "prettier": "1.13.7"
    }
}
