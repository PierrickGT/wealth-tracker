{
  "name": "WealthTracker",
  "version": "1.0.0",
  "description": "A simple interface to track my wealth",
  "main": "index.js",
  "scripts": {
    "doc": "./node_modules/.bin/esdoc",
    "format-all": "./node_modules/.bin/prettier --write **/*.js*",
    "format-watch": "onchange '**/*.js*' -- prettier --write {{changed}}",
    "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx src",
    "start": "parcel --no-cache src/index.html",
    "test": "npm run format-all && npm run lint && jest --colors --coverage",
    "test:watch": "jest --colors --coverage --watch",
    "update": "updtr --exclude less -t 'npm run format-all && npm run lint && npm test'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format-all && npm run lint && npm run doc && npm test"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "antd": "^3.10.0",
    "apollo-boost": "^0.1.16",
    "apollo-cache-inmemory": "^1.2.10",
    "babel-plugin-styled-components": "^1.8.0",
    "bizcharts": "^3.2.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.10.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "polished": "^2.2.0",
    "prop-types": "15.6.2",
    "react": "^16.5.2",
    "react-apollo": "^2.2.4",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1",
    "styled-components": "4.1.3",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "acorn": "^6.0.5",
    "ajv": "^6.5.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "esdoc": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-jsx-plugin": "1.0.0",
    "esdoc-plugin-require-coverage": "0.1.2",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "1.3.1",
    "jest": "^23.6.0",
    "less": "2.7.3",
    "onchange": "^5.0.2",
    "parcel-bundler": "^1.10.2",
    "prettier": "^1.14.3",
    "react-router-test-context": "0.1.0",
    "react-test-renderer": "^16.5.2",
    "regenerator-runtime": "0.13.1",
    "snyk": "^1.100.2"
  }
}
