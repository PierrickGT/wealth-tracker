{
    "name": "WealthTracker",
    "version": "1.0.0",
    "description": "A simple interface to track my wealth",
    "main": "index.js",
    "scripts": {
        "doc": "./node_modules/.bin/esdoc",
        "format-all": "./node_modules/.bin/prettier --write **/*.js*",
        "format-watch": "onchange '**/*.js*' -- prettier --write {{changed}}",
        "lint": "./node_modules/.bin/eslint --ext .js --ext .jsx src",
        "start": "parcel --no-cache src/index.html",
        "test": "jest --colors --coverage",
        "test:watch": "jest --colors --coverage --watch"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format-all && npm run lint && npm run doc"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "antd": "3.6.5",
        "bizcharts": "3.1.10",
        "enzyme": "3.5.0",
        "enzyme-adapter-react-16": "1.3.1",
        "prop-types": "15.6.2",
        "react": "16.4.1",
        "react-dom": "16.4.2",
        "react-hot-loader": "4.3.3",
        "react-router-dom": "4.3.1",
        "styled-components": "3.3.3"
    },
    "devDependencies": {
        "babel-jest": "23.4.2",
        "babel-plugin-import": "1.8.0",
        "babel-plugin-istanbul": "5.0.1",
        "babel-plugin-transform-class-properties": "6.24.1",
        "babel-preset-env": "1.7.0",
        "babel-preset-react": "6.24.1",
        "esdoc": "1.1.0",
        "esdoc-ecmascript-proposal-plugin": "1.0.0",
        "esdoc-jsx-plugin": "1.0.0",
        "esdoc-plugin-require-coverage": "0.1.2",
        "esdoc-standard-plugin": "1.0.0",
        "eslint": "4.19.1",
        "eslint-config-airbnb": "17.0.0",
        "eslint-config-prettier": "2.9.0",
        "eslint-plugin-import": "2.13.0",
        "eslint-plugin-jsx-a11y": "6.1.0",
        "eslint-plugin-react": "7.10.0",
        "husky": "1.0.0-rc.13",
        "jest": "23.5.0",
        "less": "2.7.3",
        "onchange": "4.0.0",
        "parcel-bundler": "1.9.4",
        "prettier": "1.13.7",
        "react-router-test-context": "0.1.0",
        "react-test-renderer": "16.4.2"
    }
}
